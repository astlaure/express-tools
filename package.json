{
  "name": "@astlaure/express-tools",
  "version": "1.0.0",
  "description": "Express Tools to remove boilerplate",
  "main": "dist/cjs/main.js",
  "module": "dist/esm/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "rimraf dist && npm run build:src && npm run build:types",
    "build:src": "rollup -c rollup.config.mjs",
    "build:types": "dts-bundle-generator -o dist/main.d.ts src/main.ts",
    "test": "jest"
  },
  "keywords": [
    "express"
  ],
  "author": "Alexandre St-Laurent",
  "license": "ISC",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "peerDependencies": {
    "express": ">=4.19"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "dts-bundle-generator": "^9.5.1",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.17.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  }
}
